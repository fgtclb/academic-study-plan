{
    "name": "fgtclb/academic-study-plan",
    "description": "TYPO3 extension for building and displaying academic study plans with semesters, modules, and categorization features.",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "FGTCLB GmbH",
            "email": "hello@fgtclb.com"
        }
    ],
    "support": {
        "issues": "https://github.com/fgtclb/academic-extensions/issues",
        "source": "https://github.com/fgtclb/academic-extensions",
        "email": "hello@fgtclb.com"
    },
    "homepage": "https://www.fgtclb.com/",
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "fgtclb/academic-base": "2.1.2@dev",
        "typo3/cms-backend": "^12.4.22 || ^13.4",
        "typo3/cms-core": "^12.4.22 || ^13.4",
        "typo3/cms-fluid": "^12.4.22 || ^13.4",
        "typo3/cms-install": "^12.4.22 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "FGTCLB\\AcademicStudyPlan\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FGTCLB\\AcademicStudyPlan\\Tests\\": "Tests"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": true,
        "sort-packages": true
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "app-dir": ".Build",
            "extension-key": "academic_study_plan"
        },
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    }
}
